<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>provider</servlet-name>
        <servlet-class>generadorjava.genorariojava.resources.provider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkerArea</servlet-name>
        <servlet-class>generadorjava.genorariojava.resources.checkerArea</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loader</servlet-name>
        <servlet-class>generadorjava.genorariojava.resources.loader</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>generateSchedule</servlet-name>
        <servlet-class>generadorjava.genorariojava.resources.generateSchedule</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>provider</servlet-name>
        <url-pattern>/provider</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkerArea</servlet-name>
        <url-pattern>/checkerArea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loader</servlet-name>
        <url-pattern>/loader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>generateSchedule</servlet-name>
        <url-pattern>/generateSchedule</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
